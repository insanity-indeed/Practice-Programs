#include <iostream>
#include <math.h>


using namespace std;

int DeciToBin(int n){
    
    int ans =0, i =0;
    while (n != 0){
        int bits = n&1;
         ans = (bits * pow(10,i)) + ans;
         n=n>>1;
         i++;   
    }
        
    return ans;
}

int countOne(int a){   
    int count=0;
    int binary = DeciToBin(a);
    while(binary != 0 ){
        if (binary%10 ==1 )   
            count++ ;
        
        binary = binary / 10;
    }
    return count ;
}

int calBits(int a , int b){
    int countA  = countOne(a);
    int countB = countOne(b);
    return countA+countB;
}

int main()
{
    int a,b;
    cin>>a>>b;
    cout << "The nos of 1's are : " << calBits(a,b);
    return 0;
}
