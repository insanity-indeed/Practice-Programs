#include <iostream>

using namespace std;

void max_min(int a[],int n){
    int min = int_MAX, max=int_MIN;
    for(int i=0 ; i< n; i++){
        if(max<a[i+1])
            max = a[i+1];
            
        if(min>a[i+1])
            min = a[i+1];
    }
    cout<<"Maximum : "<<max<<endl;
    cout<<"Minimum : "<<min;
    
}

int main()
{
    
    int arr[10] = {0,1,2,3};
    max_min(arr,4);
    return 0;
}
