#include <iostream>

using namespace std;

void bubbleSort(int arr[], int size){ 
 
    for(int i = 0; i<size; i++){
    
        for(int j = 0; j < size - i ; j++){
            if(arr[j] > arr[j+1])
                swap(arr[j],arr[j+1]);
        }
    }

    for(int i = 0; i< size; i++){
        cout << arr[i] << "\t" ;
    }

}

int main()
{
    int arr[5] = {15,22,4,1,19};
    int size = sizeof(arr)/sizeof(int);

    bubbleSort(arr,size);

    return 0;
}
