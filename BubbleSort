#include <iostream>

using namespace std;

void bubbleSort(int arr[], int size){ 
    int last = size;   
    for(int i = 0; i<last; i++){
    
        for(int j = 1; j < last ; j++){
            if(arr[j-1] > arr[j])
                swap(arr[i],arr[j]);
        }
        last--;
    }

    for(int i = 0; i< size; i++){
        cout << arr[i] << "\t" ;
    }

}

int main()
{
    int arr[5] = {15,22,4,1,19};
    int size = sizeof(arr)/sizeof(int);

    bubbleSort(arr,size);

    return 0;
}
