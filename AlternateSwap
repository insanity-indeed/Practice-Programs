#include <iostream>

using namespace std;

void print(int arr[], int size){
    for(int i=0; i< size ; i++){
        cout<< arr[i] << " " ;         
    }
}

void alterswap(int arr[], int size){
   for(int i=0; i<size;i=i+2){
           if(i+1<size)
            swap(arr[i],arr[i+1]);
    }
}

int main()
{
    int size ;
    cout << "Enter the Size : " ;
    cin >> size ;
    
    int arr[100];
    
    cout << "Enter the Array :" ;
    
    for(int i=0; i<size ; i++){
        cin>> arr[i];
    }
    
    alterswap(arr,size);
    
    print(arr,size);

    return 0;
}
